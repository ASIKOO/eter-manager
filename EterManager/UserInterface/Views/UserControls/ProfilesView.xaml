<UserControl x:Class="EterManager.UserInterface.Views.UserControls.ProfilesView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="clr-namespace:System.Windows.Controls;assembly=MultiSelectTreeView">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="175*"/>
            <ColumnDefinition Width="274*"/>
        </Grid.ColumnDefinitions>
        <ListBox Margin="10,31,9.8,10.2" ItemsSource="{Binding ProfileList}">
            
        </ListBox>
        <TextBlock Grid.Column="0" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Profiles available:" VerticalAlignment="Top" Height="16" Width="91"/>
        <GroupBox Grid.Column="1" Header="Selected Profile" Margin="10,10,7,9">
            <Grid>
                <TextBox Height="23" Margin="66,10,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
